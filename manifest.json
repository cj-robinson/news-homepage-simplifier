{
    "manifest_version": 3,
    "name": "Iowa Capital Dispatch - Simplified",
    "version": "1.1",
    "permissions": [
      "scripting",
      "activeTab",
      "storage"
    ],
    "host_permissions": [
      "https://iowacapitaldispatch.com/*",
      "https://cors-anywhere.herokuapp.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [
      {
        "resources": ["icon.png", "redirect.html"],
        "matches": ["<all_urls>"]
      }
    ],
  "icons": {
    "48": "icon.png"
  },
    "action": {
      "default_popup": "popup.html"
    }
}